EN ENT.MUNICIPIO &lt;&gt; ' ' THEN ENT.MUNICIPIO ELSE EMP.municipio END ENTREGA_MUNICIPIO,&#13;&#10;    CASE WHEN ENT.UF &lt;&gt; ' ' THEN ENT.UF ELSE EMP.uf END ENTREGA_UF,&#13;&#10;    CASE WHEN ENT.BAIRRO &lt;&gt; ' ' THEN ENT.BAIRRO ELSE EMP.bairro END ENTREGA_BAIRRO,&#13;&#10;    CASE WHEN ENT.CEP IS NOT NULL THEN ENT.CEP ELSE EMP.cep END ENTREGA_CEP,&#13;&#10;    CASE WHEN ENT.CAMPO20 &lt;&gt; ' ' THEN ENT.CAMPO20 ELSE EMP.numero END ENTREGA_NUM,&#13;&#10;    CASE WHEN ENT.CAMPO21 &lt;&gt; ' ' THEN ENT.CAMPO21 ELSE EMP.complemento END ENTREGA_COMPLEMENTO,&#13;&#10;    &#13;&#10;&#13;&#10;    /********** DADOS PESSOAIS **********/&#13;&#10;    FIS.cargo,&#13;&#10;    CASE FIS.estado_civil&#13;&#10;      WHEN 'C' THEN 'Casado'&#13;&#10;      WHEN 'D' THEN 'Divorciado'&#13;&#10;      WHEN 'F' THEN 'Falecido'&#13;&#10;      WHEN 'S' THEN 'Solteiro'&#13;&#10;      WHEN 'U' THEN 'Com União Estável'&#13;&#10;      WHEN 'V' THEN 'Viúvo'&#13;&#10;    END AS ESTADO_CIVIL,&#13;&#10;    FIS.RG,&#13;&#10;    FIS.ORGAO_EXPEDIDOR,&#13;&#10;    FIS.NOME_PAI,&#13;&#10;    FIS.NOME_MAE,    &#13;&#10;    CASE&#13;&#10;      WHEN FIS.NACIONALIDADE IS NULL THEN 'Brasileiro'&#13;&#10;      WHEN FIS.NACIONALIDADE = ' ' THEN 'Brasileiro'&#13;&#10;    ELSE FIS.NACIONALIDADE END NACIONALIDADE,    &#13;&#10;    VEC.EMAIL,&#13;&#10;&#13;&#10;&#13;&#10;    /********** DADOS DO ADMINISTRADOR **********/&#13;&#10;    (SELECT ADM.nome_completo FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_nome_completo,&#13;&#10;    (SELECT&#13;&#10;        CASE ADM.pessoa&#13;&#10;        WHEN 1 THEN (SELECT regexp_replace(LPAD(ADM.cnpj_cpf, 11, '0'), '([0-9]{3})([0-9]{3})([0-9]{3})([0-9]{2})','\1.\2.\3-\4') FROM dual)&#13;&#10;        ELSE (SELECT regexp_replace(LPAD(ADM.cnpj_cpf, 14, '0'),'([0-9]{2})([0-9]{3})([0-9]{3})([0-9]{4})([0-9]{2})','\1.\2.\3/\4-\5') FROM dual)&#13;&#10;        END&#13;&#10;     FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR