THEN 'Solteiro'&#13;&#10;        WHEN 'U' THEN 'Com União Estável'&#13;&#10;        WHEN 'V' THEN 'Viúvo'&#13;&#10;      END&#13;&#10;     FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_estado_civil,&#13;&#10;    (SELECT F.RG FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_RG,&#13;&#10;    (SELECT F.ORGAO_EXPEDIDOR FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_ORGAO_EXPEDIDOR,&#13;&#10;    (SELECT F.NOME_PAI FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_NOME_PAI,&#13;&#10;    (SELECT F.NOME_MAE FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_NOME_MAE,&#13;&#10;    (SELECT&#13;&#10;      CASE&#13;&#10;        WHEN F.NACIONALIDADE IS NULL THEN 'Brasileiro'&#13;&#10;        WHEN F.NACIONALIDADE = ' ' THEN 'Brasileiro'&#13;&#10;        ELSE F.NACIONALIDADE&#13;&#10;        END&#13;&#10;     FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_NACIONALIDADE,&#13;&#10;    &#13;&#10;    (SELECT V.EMAIL FROM VECEMPRE V WHERE V.CD_EMPRESA = CON.RESPONSAVEL) AS ADMINISTRADOR_EMAIL,&#13;&#10;    &#13;&#10;&#13;&#10;&#13;&#10;    /********** DADOS DO FIADOR **********/&#13;&#10;    (SELECT ADM.nome_completo FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_nome_completo,&#13;&#10;    (SELECT&#13;&#10;        CASE ADM.pessoa&#13;&#10;        WHEN 1 THEN (SELECT regexp_replace(LPAD(ADM.cnpj_cpf, 11, '0'), '([0-9]{3})([0-9]{3})([0-9]{3})([0-9]{2})','\1.\2.\3-\4') FROM dual)&#13;&#10;        ELSE (SELECT regexp_replace(LPAD(ADM.cnpj_cpf, 14, '0'),'([0-9]{2})([0-9]{3})([0-9]{3})([0-9]{4})([0-9]{2})','\1.\2.\3/\4-\5') FROM dual)&#13;&#10;        END&#13;&#10;     FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_cnpj_cpf,&#13;&#10;    &#13;&#10;    (SELECT ADM.endereco FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_endereco,&#13;&#10;    (SELECT ADM.numero FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_numero,&#13;&#10;    (SELECT ADM.complemento FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_complemento,&#13;&#10;    (SELECT ADM.bairro FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_bairro,&#13;&#10;    (SELECT ADM.municipio FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_municipio,&#13;&#10;    (SELECT ADM.uf FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_uf,&#13;&#10;    (SELECT ADM.cep FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_cep,&#13;&#10;    (SELECT ADM.contato FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_contato,&#13;&#10;    (SELECT ADM.fone FROM GEEMPRES ADM WHERE ADM.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_fone,&#13;&#10;    &#13;&#10;    (SELECT  &#13;&#10;      CASE WHEN EXISTS(SELECT TELEFONE FROM GENTEL WHERE EMPRESA = CON.CD_FIADOR)&#13;&#10;        THEN (SELECT LISTAGG(TELEFONE, ' / ') WITHIN GROUP (ORDER BY TELEFONE) FROM GENTEL WHERE EMPRESA = CON.CD_FIADOR)&#13;&#10;        ELSE (SELECT TELEFONE FROM GENTEL WHERE EMPRESA = CON.CD_FIADOR)&#13;&#10;      END&#13;&#10;      FROM dual) AS FIADOR_TELEFONE,&#13;&#10;&#13;&#10;    /********** DADOS PESSOAIS DO FIADOR **********/&#13;&#10;    (SELECT F.cargo FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_cargo,&#13;&#10;    (SELECT&#13;&#10;      CASE F.estado_civil&#13;&#10;        WHEN 'C' THEN 'Casado'&#13;&#10;        WHEN 'D' THEN 'Divorciado'&#13;&#10;        WHEN 'F' THEN 'Falecido'&#13;&#10;        WHEN 'S' THEN 'Solteiro'&#13;&#10;        WHEN 'U' THEN 'Com União Estável'&#13;&#10;        WHEN 'V' THEN 'Viúvo'&#13;&#10;      END&#13;&#10;     FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_estado_civil,&#13;&#10;    (SELECT F.RG FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_RG,&#13;&#10;    (SELECT F.ORGAO_EXPEDIDOR FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_ORGAO_EXPEDIDOR,&#13;&#10;    (SELECT F.NOME_PAI FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_NOME_PAI,&#13;&#10;    (SELECT F.NOME_MAE FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_NOME_MAE,&#13;&#10;    (SELECT&#13;&#10;      CASE&#13;&#10;        WHEN F.NACIONALIDADE IS NULL THEN 'Brasileiro'&#13;&#10;        WHEN F.NACIONALIDADE = ' ' THEN 'Brasileiro'&#13;&#10;        ELSE F.NACIONALIDADE&#13;&#10;        END&#13;&#10;     FROM GEPFISIC F WHERE F.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_NACIONALIDADE,&#13;&#10;    &#13;&#10;    (SELECT V.EMAIL FROM VECEMPRE V WHERE V.CD_EMPRESA = CON.CD_FIADOR) AS FIADOR_EMAIL,&#13;&#10;&#13;&#10;&#13;&#10;    /********** DADOS DA UNIDADE DE NEGOCIO **********/&#13;&#10;    -- (SELECT A.nome_completo FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_NOME,&#13;&#10;    'ALIANÇA ASSESSORIA DE CRÉDITO EIRELI' AS UNIDADE_NOME,&#13;&#10;    (SELECT regexp_replace(LPAD((SELECT a.cnpj_cpf FROM GEEMPRES A WHERE A.CD_EMPRESA = '1'), 14, '0'),'([0-9]{2})([0-9]{3})([0-9]{3})([0-9]{4})([0-9]{2})','\1.\2.\3/\4-\5') FROM dual) AS UNIDADE_CNPJ,&#13;&#10;    (SELECT a.endereco FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_ENDERECO,&#13;&#10;    (SELECT a.numero FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_NUMERO,&#13;&#10;    (SELECT a.complemento FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_COMPLEMENTO,&#13;&#10;    (SELECT a.bairro FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_BAIRRO,&#13;&#10;    (SELECT a.municipio FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_MUNICIPIO,&#13;&#10;    (SELECT a.uf FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_UF,&#13;&#10;    (SELECT a.cep FROM GEEMPRES A WHERE A.CD_EMPRESA = '1') AS UNIDADE_CEP,&#13;&#10;    &#13;&#10;    &#13;&#10;    &#13;&#10;    &#13;&#10;    &#13;&#10;    /**********  CONTRATO DE SERVICO **********/&#13;&#10;    CON.CD_CONTRATO AS SERVICO_CD_CONTRATO,&#13;&#10;    (SELECT HISTORICO FROM GEACOMP WHERE CONTATO_OS_LANC = CON.CD_CONTRATO AND CODIGO_TITULO = 'CNH' AND ROWNUM = 1) AS INFRACAO,&#13;&#10;    CON.OBSERVACAO AS ORGAO_CNH,&#13;&#10;    TO_CHAR(CON.DT_INICIO, 'DD/MM/YYYY') AS SERVICO_DT_INICIO,&#13;&#10;    -- TO_CHAR(CON.DT_INICIO,'Day') AS DIA_SEMANA,&#13;&#10;    TO_CHAR(CON.DT_INICIO,'DD') AS SERVICO_DIA,&#13;&#10;    TO_CHAR(CON.DT_INICIO,'Month','nls_date_language =''brazilian portuguese''') AS SERVICO_MES,&#13;&#10;    TO_CHAR(CON.DT_INICIO,'YYYY') AS SERVICO_ANO,    &#13;&#10;&#13;&#10;    CON.DT_RENOVACAO AS DATA_VALIDADE_PROCURACAO,&#13;&#10;    &#13;&#10;    CON.CAMPO75 AS SERVICO_QUANTIDADE_PARCELAS_ATRASO,&#13;&#10;    &#13;&#10;    CASE&#13;&#10;      WHEN CON.CAMPO75 &lt;&gt; ' ' THEN NUMERO_POR_EXTENSO(CON.CAMPO75)&#13;&#10;      ELSE ' '&#13;&#10;    END AS SERVICO_QUANTIDADE_PARCELAS_ATRASO_EXTENSO,&#13;&#10;    &#13;&#10;    CASE&#13;&#10;      WHEN CON.CAMPO75 = ' ' THEN 'NÃO'&#13;&#10;      ELSE ' '&#13;&#10;    END AS SERVICO_CONSTA_SPC_SERASA,    &#13;&#10;    (SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND ROWNUM = 1) AS SERVICO_VALOR_ENTRADA,&#13;&#10;    VALOR_POR_EXTENSO((SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND ROWNUM = 1)) AS SERVICO_VALOR_ENTRADA_EXTENSO,&#13;&#10;    TO_CHAR((SELECT * FROM (SELECT DT_VENCIMENTO FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO ORDER BY cd_lancamento) WHERE ROWNUM=1), 'DD/MM/YYYY') AS SERVICO_ENTRADA_VENCIMENTO,&#13;&#10;    -- (SELECT DT_VENCIMENTO FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND ROWNUM =1) AS SERVICO_ENTRADA_VENCIMENTO,&#13;&#10;    (SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_LANCAMENTO = (SELECT MAX(CD_LANCAMENTO) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_C_PARTIDA = '0' )) AS SERVICO_VALOR_DEMAIS_PARCELAS,&#13;&#10;    VALOR_POR_EXTENSO((SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_LANCAMENTO = (SELECT MAX(CD_LANCAMENTO) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO))) AS SERVICO_VALOR_DEMAIS_PARCELAS_EXTENSO,&#13;&#10;    (SELECT EXTRACT(DAY FROM (MAX(DT_VENCIMENTO))) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO) AS SERVICO_DEMAIS_PARCELAS_VENCIMENTO,&#13;&#10;    (SELECT COUNT(CD_LANCAMENTO) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_TIPO &lt;&gt; 'E') AS SERVICO_QUATIDADE_TOTAL_PARCELAS,&#13;&#10;    &#13;&#10;    (SELECT SUM(VALOR) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_TIPO &lt;&gt; 'E') AS SERVICO_VALOR_TOTAL,&#13;&#10;    VALOR_POR_EXTENSO((SELECT SUM(VALOR) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO AND CD_TIPO &lt;&gt; 'E')) AS SERVICO_VALOR_TOTAL_EXTENSO,&#13;&#10;    &#13;&#10;    &#13;&#10;    /********** CONTRATO DE REDUZIDAS **********/&#13;&#10;    (SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND ROWNUM = 1) AS REDUZIDAS_VALOR_ENTRADA,&#13;&#10;    VALOR_POR_EXTENSO((SELECT VALOR FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND ROWNUM = 1)) AS REDUZIDAS_VALOR_ENTRADA_EXTENSO,&#13;&#10;    TO_CHAR((SELECT DT_VENCIMENTO FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND ROWNUM = 1), 'DD/MM/YYYY') AS REDUZIDAS_ENTRADA_VENCIMENTO,&#13;&#10;    TO_CHAR((SELECT DT_VENCIMENTO FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND ROWNUM = 1), 'DD') AS REDUZIDAS_ENTRADA_VENCIMENTO_DIA,&#13;&#10;    (SELECT COUNT(CD_LANCAMENTO) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND CD_TIPO &lt;&gt; 'E') AS REDUZIDAS_QUATIDADE_TOTAL_PARCELAS,&#13;&#10;    (SELECT SUM(VALOR) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND CD_TIPO &lt;&gt; 'E') AS REDUZIDAS_VALOR_TOTAL,&#13;&#10;    VALOR_POR_EXTENSO((SELECT SUM(VALOR) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND CD_TIPO &lt;&gt; 'E')) AS REDUZIDAS_VALOR_TOTAL_EXTENSO,&#13;&#10;    &#13;&#10;    &#13;&#10;    &#13;&#10;    /********** CONFISSAO DE DIVIDA **********/&#13;&#10;    (SELECT SUM(L.VALOR) FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R') AS CONFISSAO_VALOR_TOTAL_RESTANTE,&#13;&#10;    VALOR_POR_EXTENSO((SELECT SUM(L.VALOR) FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R')) AS CONFISSAO_VALOR_TOTAL_RESTANTE_EXTENSO,&#13;&#10;    (SELECT COUNT(L.CD_LANCAMENTO) FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R') AS CONFISSAO_QUATIDADE_TOTAL_PARCELAS,&#13;&#10;    NUMERO_POR_EXTENSO((SELECT COUNT(L.CD_LANCAMENTO) FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R')) AS CONFISSAO_QUATIDADE_TOTAL_PARCELAS_EXTENSO,&#13;&#10;    (SELECT L.VALOR FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R' AND ROWNUM = 1) AS CONFISSAO_VALOR_PARCELA,&#13;&#10;    VALOR_POR_EXTENSO((SELECT L.VALOR FROM GFLANCAM L WHERE L.DOCUMENTO = CON.CD_CONTRATO_REP AND L.CD_TIPO = 'R' AND ROWNUM = 1)) AS CONFISSAO_VALOR_PARCELA_EXETENSO,&#13;&#10;    TO_CHAR((SELECT DT_VENCIMENTO FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND CD_TIPO = 'R' AND ROWNUM = 1), 'DD/MM/YYYY') AS CONFISSAO_PRIMEIRO_VENCIMENTO,&#13;&#10;    TO_CHAR((SELECT MAX(DT_VENCIMENTO) FROM GFLANCAM WHERE DOCUMENTO = CON.CD_CONTRATO_REP AND CD_TIPO = 'R'), 'DD/MM/YYYY') AS CONFISSAO_ULTIMO_VENCIMENTO,&#13;&#10;&#13;&#10;&#13;&#10;    /********** DADOS DO ORCAMENTO **********/&#13;&#10;    PED.embarque AS BANCO,&#13;&#10;    CASE WHEN PED.QUANTIDADE &lt;&gt; ' ' THEN TO_NUMBER(PED.QUANTIDADE) * PED.PESO_LIQUIDO ELSE 0 END TOTAL_ORIGINAL,&#13;&#10;    CASE WHEN PED.QUANTIDADE &lt;&gt; ' ' THEN VALOR_POR_EXTENSO(TO_NUMBER(PED.QUANTIDADE) * PED.PESO_LIQUIDO) ELSE ' ' END TOTAL_ORIGINAL_EXTENSO,&#13;&#10;    TO_NUMBER(PED.QUANTIDADE) AS QUANTIDADE_TOTAL_PARCELAS,&#13;&#10;    CASE WHEN PED.QUANTIDADE &lt;&gt; ' ' THEN NUMERO_POR_EXTENSO(PED.QUANTIDADE) ELSE ' ' END QUANTIDADE_TOTAL_PARCELAS_EXTENSO,&#13;&#10;    TO_NUMBER(PED.ESPECIE) AS QUANTIDADE_PARCELAS_PAGAS,&#13;&#10;    CASE WHEN PED.ESPECIE &lt;&gt; ' ' THEN NUMERO_POR_EXTENSO(PED.ESPECIE) ELSE ' ' END QUANTIDADE_PARCELAS_PAGAS_EXTENSO,&#13;&#10;    CASE WHEN PED.ESPECIE &lt;&gt; ' ' AND PED.QUANTIDADE &lt;&gt; ' ' THEN TO_NUMBER(PED.QUANTIDADE) - TO_NUMBER(PED.ESPECIE) ELSE 0 END QUANTIDADE_PARCELAS_FALTANTES,&#13;&#10;    CASE WHEN PED.ESPECIE &lt;&gt; ' ' AND PED.QUANTIDADE &lt;&gt; ' ' THEN NUMERO_POR_EXTENSO(TO_NUMBER(PED.QUANTIDADE) - TO_NUMBER(PED.ESPECIE)) ELSE ' ' END QUANTIDADE_PARCELAS_FALTANTES_EXTENSO,&#13;&#10;    PED.PESO_LIQUIDO AS VALOR_ORIGINAL_PARCELA,&#13;&#10;     CASE WHEN PED.PESO_LIQUIDO &lt;&gt; 0 THEN VALOR_POR_EXTENSO(PED.PESO_LIQUIDO) ELSE ' ' END VALOR_ORIGINAL_PARCELA_EXTENSO,&#13;&#10;    PED.CAMPO91 AS DATA_FINANCIAMENTO,&#13;&#10;    PED.DESTINO AS NUMERO_CONTRATO_FINANCIAMENTO,&#13;&#10;    PED.VIA_DE_EMBARQUE AS REU,&#13;&#10;    &#13;&#10;    CASE WHEN EXISTS (SELECT OBSERVACAO FROM FAOIPEDI OBS WHERE OBS.CD_PEDIDO = CON.CD_PEDIDO AND TIPO = 'O' AND ROWNUM = 1) THEN&#13;&#10;      (SELECT OBSERVACAO FROM FAOIPEDI OBS WHERE OBS.CD_PEDIDO = CON.CD_PEDIDO AND TIPO = 'O' AND ROWNUM = 1)&#13;&#10;    ELSE&#13;&#10;      EMP.NOME_COMPLETO&#13;&#10;    END AS TITULAR_NOME,&#13;&#10;    (SELECT OBSERVACAO FROM FAOIPEDI OBS WHERE OBS.CD_PEDIDO = CON.CD_PEDIDO AND TIPO = 'P' AND ROWNUM = 1) AS TITULAR_CPF,&#13;&#10;    (SELECT OBSERVACAO FROM FAOIPEDI OBS WHERE OBS.CD_PEDIDO = CON.CD_PEDIDO AND TIPO = 'E' AND ROWNUM = 1) AS TITULAR_RG,&#13;&#10;    (SELECT OBSERVACAO FROM FAOIPEDI OBS WHERE OBS.CD_PEDIDO = CON.CD_PEDIDO AND TIPO = 'F' AND ROWNUM = 1) AS TITULAR_DT_NASCIMENTO,&#13;&#10;&#13;&#10;&#13;&#10;    /********** DADOS DO VEICULO **********/&#13;&#10;    VEI.PLACA AS PLACA,&#13;&#10;    VEI.ANO_FABRICACAO,&#13;&#10;    VEI.ANO_MODELO,&#13;&#10;    VEI.COMBUSTIVEL AS MODELO_COMPLEMENTO,&#13;&#10;    VEI.COR,&#13;&#10;    VEI.CHASSI AS CHASSIS,&#13;&#10;    VEI.RENAVAM,&#13;&#10;    (SELECT DESCRICAO FROM GVVEIMAR WHERE ID = VEI.CAMPO72) AS MARCA,&#13;&#10;    (SELECT DESCRICAO FROM GVVEIMOD WHERE ID = VEI.CAMPO73) AS MODELO,&#13;&#10;    VEI.PESO_BRUTO_TOT AS AGIO_COMPRA,&#13;&#10;    VEI.TAR